APP_NAME=kue-intelligence
APP_ENV=development
APP_HOST=0.0.0.0
APP_PORT=8000

# Google OAuth callback completion settings
GOOGLE_OAUTH_CLIENT_ID=your-google-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-client-secret
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8000/v1/ingestion/google/oauth/callback

# Layer 2 raw capture store
RAW_EVENTS_DB_PATH=data/raw_events.db
CANONICAL_EVENTS_DB_PATH=data/canonical_events.db
PARSER_VERSION=v1

# Supabase (Layer 2 preferred store when configured)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_RAW_EVENTS_TABLE=raw_events
SUPABASE_CANONICAL_EVENTS_TABLE=canonical_events

# Inngest event stream (for stage/layer execution visibility)
INNGEST_EVENT_KEY=your-inngest-event-key
INNGEST_SIGNING_KEY=your-inngest-signing-key
INNGEST_BASE_URL=https://inn.gs
INNGEST_SOURCE_APP=kue-intelligence
INNGEST_MAX_RETRIES=5

# Optional alerting webhook called when all retries are exhausted and a function still fails
ALERT_WEBHOOK_URL=
